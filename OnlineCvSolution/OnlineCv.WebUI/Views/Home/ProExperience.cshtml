@model IEnumerable<OnlineCv.WebUI.Models.Entity.ProfessionalExperience>
@{
    Layout = null;
}
<!-- Professional Experience -->
<div class="inside-sec margin-top-30">
    <!-- Professional Experience -->
    <h5 class="tittle">Professional Experience</h5>
    <div class="padding-30 exp-history">
        @foreach (var item in Model)
        {
            <!-- Wordpress Developer -->
            <div class="exp">
                <div class="media-left"> <span class="sun">@item.Duration</span> </div>
                <div class="media-body">

                    <!-- COmpany Logo -->
                    <div class="company-logo" style="width:100px;height:100px"> <img src="~/Template/media/@item.MediaUrl" alt=""> </div>
                    <h6>@item.JobTitle</h6>
                    <p>@item.CompanyName</p>
                    <p>@item.Location</p>
                    <p class="margin-top-10"> @item.Details </p>
                </div>
            </div>
        }
    </div>
</div>
